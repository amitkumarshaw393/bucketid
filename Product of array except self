/**Input: nums = [1,2,3,4]
Output: [24,12,8,6]**/

#include <bits/stdc++.h>
#include <algorithm>

using namespace std;

int main(void)
{
  
        vector<int>nums{-1,1,0,-3,3};
        int res;
        vector<int>v1,v2;
        for(int i=0;i<nums.size();i++){
            res=1;
            for(int j=0;j<nums.size();j++){
           v1.push_back(nums[j]);
              
        }
         v1.erase(v1.begin()+i);
         for(int k=0;k<v1.size();k++)
         {
             res*=v1[k];
         }
            v1.clear();
            v2.push_back(res);
                
        }
        for(auto y:v2)
        cout<<y<<endl;
 
}
